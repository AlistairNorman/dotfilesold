[alias]
    head = !git l -1
    d = diff --color
    h = !git head
    hp = "!source ~/.githelpers && show_git_head"
    r = !git l -30
    ra = !git r --all
    ff = merge --ff-only
    pullff = pull --ff-only
    noff = merge --no-ff
    l = "!source ~/.githelpers && pretty_git_log"
    la = !git l --all
